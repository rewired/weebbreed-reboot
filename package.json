{
  "name": "weebbreed-reboot",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "tsx --env-file=.env src/server/index.ts",
    "dev:client": "vite",
    "build": "npm-run-all build:server build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "lint": "echo 'No linter configured'",
    "test": "vitest",
    "bench": "node dist/server/benchmarks/runBenchmarks.js"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.2",
    "i18next": "^25.5.2",
    "mathjs": "^14.7.0",
    "npm-run-all": "^4.1.5",
    "pino": "^9.10.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "recharts": "^3.2.1",
    "rxjs": "^7.8.2",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "xstate": "^5.22.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/seedrandom": "^3.0.8",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  }
}
